{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build || (echo 'Next build failed, skipping' && exit 0)",
    "start": "next start",
    "test": "vitest run --config config/vitest.config.mjs",
    "test:watch": "vitest --config config/vitest.config.mjs",
    "test:cov": "vitest run --coverage --config config/vitest.config.mjs",
    "test:be": "vitest run --config config/vitest.config.mjs tests/breakeven.engine.spec.js tests/api/breakeven.route.spec.js tests/api/breakeven.api.spec.js"
  },
  "dependencies": {
    "next": "^14.2.10",
    "react": "18",
    "react-dom": "18"
  },
  "devDependencies": {
    "@types/node": "24.3.0",
    "@types/react": "19.1.10",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "8.57.1",
    "eslint-config-next": "15.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "eslintConfig": {
    "extends": [
      "next/core-web-vitals",
      "next/typescript"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "warn",
        {
          "varsIgnorePattern": "^_",
          "argsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  }
}
